datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Report {
  id          Int      @id @default(autoincrement())
  district    String
  institution String
  month       String
  year        String
  answers     Json
  cumulative  Json
  eyeBank        Json     
  visionCenter   Json     
 createdAt    DateTime @default(now())
 }
model User {
  id           Int     @id @default(autoincrement())
  district     String
  institution  String
  phone        String
}

